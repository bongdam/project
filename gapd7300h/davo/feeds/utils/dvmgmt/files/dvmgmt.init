#!/bin/sh /etc/rc.common

START=45
STOP=09

USE_PROCD=1
PROG=/usr/sbin/dvmgmt

start_service() {
	procd_open_instance
	procd_set_param command $PROG -f
	procd_set_param respawn
	procd_close_instance
}

stop_service() {
	service_stop ${PROG}
}

reload_service() {
	rc_procd stop_service "$@"
	rc_procd start_service "$@"
	return 0
}

