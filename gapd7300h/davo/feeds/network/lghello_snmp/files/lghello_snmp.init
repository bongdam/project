#!/bin/sh /etc/rc.common

START=98

USE_PROCD=1
PROG=/usr/sbin/lghello_snmp

start_service() {
	procd_open_instance
	procd_set_param command ${PROG} -a s
	procd_close_instance
}

reload_service()
{
	rc_procd stop_service "$@"
	rc_procd start_service "$@"
	return 0
}

stop_service() {
	service_stop ${PROG}
}
