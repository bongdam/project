		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			""
		::= {sHubSecurityConfig 1}

sHubSecConfigEntry 		OBJECT-TYPE
		SYNTAX 		SHUBSecConfigEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			""
		INDEX	{sHubSecConfigSSIDIndex}
		::= {sHubSecConfigTable 1}

SHUBSecConfigEntry ::=
	SEQUENCE {
		sHubSecConfigSSIDIndex	INTEGER,
		sHubSecSSID		OCTET STRING,
		sHubSecRadiusServerIp	IpAddress,
		sHubSecRadiusServerPort  	INTEGER,
		sHubSecRadiusServerPw  	OCTET STRING,
		sHubSecAccountMode  	INTEGER,
		sHubSecAccountServerIp	IpAddress,
		sHubSecAccountServerPort  	INTEGER,
		sHubSecAccountServerPw  	OCTET STRING
		}

sHubSecConfigSSIDIndex		OBJECT-TYPE
		SYNTAX 		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			""
		::= {sHubSecConfigEntry 1}

