SKB-H724G-MIB-0801 DEFINITIONS ::= BEGIN
	IMPORTS
		MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, Integer32, Counter32,
		Counter64, IpAddress, Unsigned32, enterprises		FROM SNMPv2-SMI
		DisplayString, MacAddress, RowStatus, TruthValue	FROM SNMPv2-TC
		MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP FROM SNMPv2-CONF
		ifIndex								FROM RFC1213-MIB;

	skBroadband  OBJECT IDENTIFIER ::= {enterprises 16183}

	skbSHUB	MODULE-IDENTITY
			LAST-UPDATED	"2009081900Z"
			ORGANIZATION	"SK Broadband"
			CONTACT-INFO	""
			DESCRIPTION		""
			::={skBroadband 3}


skbSHubBasicInfo				OBJECT IDENTIFIER ::= {skbSHUB 1}
skbSHubConfigInfo				OBJECT IDENTIFIER ::= {skbSHUB 2}
skbSHubStatus					OBJECT IDENTIFIER ::= {skbSHUB 3}
skbSHubDiag					OBJECT IDENTIFIER ::= {skbSHUB 4}
skbSHubExtra					OBJECT IDENTIFIER ::= {skbSHUB 5}
skbSHubTrap          				OBJECT IDENTIFIER ::= {skbSHUB 6}

