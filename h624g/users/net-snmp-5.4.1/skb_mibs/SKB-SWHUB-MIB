		SYNTAX 		SEQUENCE OF SHUBDevPortEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Device Port List Table"
		::= {sHubPortConfig 2}

sHubDevPortEntry 		OBJECT-TYPE
		SYNTAX 		SHUBDevPortEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			"Devie Port List Entry "
		INDEX	{sHubDevPortIndex}
		::= {sHubDevPortTable 1}

SHUBDevPortEntry ::=
	SEQUENCE {
		sHubDevPortIndex	INTEGER,
		sHubDevPortNumber	INTEGER,
		sHubDevPortName 	OCTET STRING,
		sHubDevPortNego	INTEGER,
		sHubDevPortSpeed	INTEGER,
		sHubDevPortDuplex	INTEGER,
		sHubDevPortOnOff	INTEGER,
		sHubDevPortReset	INTEGER
		}

sHubDevPortIndex		OBJECT-TYPE
		SYNTAX 		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The index of device port list."
		::= {sHubDevPortEntry 1}

sHubDevPortNumber		OBJECT-TYPE
		SYNTAX 		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The port number of device port list."
		::= {sHubDevPortEntry 2}

sHubDevPortName			OBJECT-TYPE
		SYNTAX 		OCTET STRING
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			"The port name of device port list."
		::= {sHubDevPortEntry 3}

sHubDevPortNego			OBJECT-TYPE
		SYNTAX 		INTEGER { force(1), auto(2) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The port negotiation of device port list.
				1) Force
				2) Auto nego"
		::= {sHubDevPortEntry 4}

sHubDevPortSpeed		OBJECT-TYPE
		SYNTAX 		INTEGER { m10(1), m100(2), m200(3), m300(4), m500(5), g1(6) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
			"The port speed of device port list.
				1) 10Mbps
				2) 100Mbps
				3) 200Mbps
				4) 300Mbps
				5) 500Mbps
				6) 1Gbps"
		::= {sHubDevPortEntry 5}

