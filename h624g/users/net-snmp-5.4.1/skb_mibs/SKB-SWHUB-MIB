 sHubSnmpConfig				OBJECT IDENTIFIER ::= {skbSHubConfigInfo 8}
 sHubSnmpEnable				OBJECT-TYPE
 		SYNTAX 		INTEGER { enable(1), disable(2) }
 		MAX-ACCESS	read-write
 		STATUS		current
 		DESCRIPTION
 			"The enable/disable SNMP agent"
 		::= {sHubSnmpConfig 1}

    communityTable OBJECT-TYPE
            SYNTAX SEQUENCE OF CommunityTableEntry
        	MAX-ACCESS not-accessible
        	STATUS current
        	DESCRIPTION
            	"A list of CommunityTable entries."
        ::= { sHubSnmpConfig 2 }

	communityTableEntry OBJECT-TYPE
	        SYNTAX CommunityTableEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	        	"An entry containing management information applicable
                 to a particular board."
	        INDEX { communityIndex }
	        ::= { communityTable 1 }

	CommunityTableEntry ::=
        	SEQUENCE {
        	        communityIndex              INTEGER,
                	communityName               OCTET STRING,
                	communityType               INTEGER,
	                communityStatus      		INTEGER,
	                communityAdmin              INTEGER,
	                }
	communityIndex OBJECT-TYPE
	        SYNTAX INTEGER
	        MAX-ACCESS read-only
	        STATUS mandatory
	        DESCRIPTION
	           "A unique value, greater than zero, for each
               port.  It is recommended that values are assigned
               contiguously starting from 1.  The value for each
               interface sub-layer must remain constant at least from
               one re-initialization of the entity's network
               management system to the next re-initialization."
	        ::= { communityTableEntry 1 }

 	communityName  OBJECT-TYPE
		SYNTAX OCTET STRING (SIZE(1..24))
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION ""
	        ::= { communityTableEntry 2 }


	communityType OBJECT-TYPE
	    	SYNTAX INTEGER {
	            ro   (0),
	            rw   (1)
		    }
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION
	        	""
	        ::= { communityTableEntry 3 }

   	communityAdmin OBJECT-TYPE
	        SYNTAX INTEGER {
	            off  (0),
	            on   (1)
	        }
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION
	        	"community Admin.
	        	 0: off ( not active )
	        	 1: on  ( active )"
	        ::= { communityTableEntry 4 }

    trapDestinationTable OBJECT-TYPE
            SYNTAX SEQUENCE OF TrapDestinationTableEntry
        	MAX-ACCESS not-accessible
        	STATUS current
        	DESCRIPTION
            	"A list of TrapDestinationTable entries."
        ::= { sHubSnmpConfig 3 }

	trapDestinationTableEntry OBJECT-TYPE
	        SYNTAX TrapDestinationTableEntry
	        MAX-ACCESS not-accessible
	        STATUS current
	        DESCRIPTION
	        	"An entry containing management information applicable
                 to a particular board."
	        INDEX { trapDestinationIndex }
	        ::= { trapDestinationTable 1 }

	TrapDestinationTableEntry ::=
        	SEQUENCE {
        	        trapDestinationIndex              INTEGER,
                    trapDestination		      OCTET STRING,
                    trapCommunityName               OCTET STRING,
	        trapDestinationAdmin              INTEGER,
	                }
	trapDestinationIndex OBJECT-TYPE
	        SYNTAX INTEGER
	        MAX-ACCESS read-only
	        STATUS mandatory
	        DESCRIPTION
	           "A unique value, greater than zero, for each
               port.  It is recommended that values are assigned
               contiguously starting from 1.  The value for each
               interface sub-layer must remain constant at least from
               one re-initialization of the entity's network
               management system to the next re-initialization."
	        ::= { trapDestinationTableEntry 1 }

 	trapDestination  OBJECT-TYPE
		SYNTAX OCTET STRING (SIZE(1..24))
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION "Trap Destination IP or Domain"
	        ::= { trapDestinationTableEntry 2 }

 	trapCommunityName  OBJECT-TYPE
		SYNTAX OCTET STRING (SIZE(1..24))
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION "Trap Destination IP or Domain"
	        ::= { trapDestinationTableEntry 3 }

   	trapDestinationAdmin OBJECT-TYPE
	        SYNTAX INTEGER {
	            off  (0),
	            on   (1)
	        }
	        MAX-ACCESS read-write
	        STATUS current
	        DESCRIPTION
	        	"trap destination Admin.
	        	 0: off ( not active )
	        	 1: on  ( active )"
	        ::= { trapDestinationTableEntry 4 }



