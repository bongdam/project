sHubCpePing			OBJECT IDENTIFIER ::= {skbSHubDiag 3}

cpePingTable OBJECT-TYPE
    SYNTAX SEQUENCE OF CpePingEntry
    ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table of CPE ping."
    ::= { sHubCpePing 1 }

cpePingEntry OBJECT-TYPE
    SYNTAX CpePingEntry
    ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A CPE ping entry"
    INDEX { cpePingPortIndex }
    ::= { cpePingTable 1 }

CpePingEntry ::= SEQUENCE {
    cpePingTablePortIndex INTEGER,
    cpePingTableAction INTEGER,
    cpePingTableCpeMac MacAddress,
    cpePingTablePingAddress IpAddress,
    cpePingTableRttMin INTEGER,
    cpePingTableRttAvg INTEGER,
    cpePingTableRttMax INTEGER,
    cpePingTableTimeout INTEGER
    }

cpePingTablePortIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS current
    DESCRIPTION
        "Object which specifies a unique entry in the
        cpePingTable."
    ::= { cpePingEntry 1 }

cpePingTableAction  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS current
    DESCRIPTION
        ""
    ::= { cpePingEntry 2 }

cpePingTableCpeMac  OBJECT-TYPE
    SYNTAX	MacAddress
    ACCESS read-only
    STATUS current
    DESCRIPTION
        ""
    ::= { cpePingEntry 3 }

cpePingTablePingAddress  OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS current
    DESCRIPTION
        ""
    ::= { cpePingEntry 4 }

cpePingTableRttMin OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS current
    DESCRIPTION
        "ms"
    ::= { cpePingEntry 5 }

cpePingTableRttAvg  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS current
    DESCRIPTION
        "ms"
    ::= { cpePingEntry 6 }

cpePingTableRttMax OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS current
    DESCRIPTION
        "ms"
    ::= { cpePingEntry 7 }

cpePingTableTimeout  OBJECT-TYPE
    SYNTAX	INTEGER
    ACCESS read-only
    STATUS current
    DESCRIPTION
        "count of ping time out"
    ::= { cpePingEntry 8 }

cpePingTrap 	OBJECT IDENTIFIER ::= {sHubCpePing 2}
