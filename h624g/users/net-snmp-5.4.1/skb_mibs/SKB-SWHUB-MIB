-- **********************************************************************
sHubWlanConfig		OBJECT IDENTIFIER ::= {skbSHubConfigInfo 3}
sHubWlanBasicConfig   OBJECT IDENTIFIER ::= {sHubWlanConfig 1}

sHubWlanMode			OBJECT-TYPE
		SYNTAX		INTEGER { enable(1), disable(2) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Enable/Disable wireless lan interface.

					Input Value
					===========
					1: enable
					2: disable"

		::= {sHubWlanBasicConfig 1}

sHubWlanBand			OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"Select wireless lan support mode.
						Value is combination of frequency
						and supported bandwidth.

						Input Value
						===========
						1: 2.4Ghz (B)
						2: 2.4Ghz (G)
						3: 2.4Ghz (N)
						4: 2.4Ghz (B+G)
						5: 2.4Ghz (G+N)
						6: 2.4Ghz (B+G+N)
						7: 2.4Ghz (B+G+N) 5.0Ghz (N)
						8: 5.0Ghz (N)"

		::= {sHubWlanBasicConfig 2}

sHubWlanChannelWidth	OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"Bandwidth per channel. This object
						is valid in case 'sHubWlanBand' is
						configured B or G or B+G.

						Input Value
						===========
						1: 20MHz
						2: 40MHz"

		::= {sHubWlanBasicConfig 3}

sHubWlanCtrlSideband	OBJECT-TYPE
		SYNTAX			INTEGER { upper(1), lower(2) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"This object
						is valid in case 'sHubWlanBand' is
						configured B or G or B+G.

						Input Value
						===========
						1: upper
						2: lower"

		::= {sHubWlanBasicConfig 4}


sHubWlanChannelNumber	OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"Input range of this object is from 0 to 11. Channel
						number 1-4 is invalid in case 'sHubWlanBand' is configured
						N or G+N or B+G+N. Input Value '0' is  'auto'."

		::= {sHubWlanBasicConfig 5}

sHubWlanDataRate	OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"You can configure Date Rate. Configuration value
						is different following 'sHubWlanBand'

						Valid Value
						===========
						Band(B) : auto, 1M, 2M, 5.5M, 11M
						Band(G) : auto, 6M, 9M, 12M, 18M(9), 24M(10), 36M(11),
								48M(12), 54M(13)
						Band(N) : MCS0, MCS1, MCS2, MCS3, MCS4, MCS5, MCS6, MCS7,
								MCS8, MCS9, MCS10, MCS11, MCS12, MCS13, MCS14, MCS15
						Band(B+G) : Band(B) + Band(G)
						Band(G+N) : Band(G) + Band(N)
						Band(B+G+N) : Band(B) + Band(G) + Band(N)"

		::= {sHubWlanBasicConfig 6}

sHubWlanSessionNum	OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-only
		STATUS			current
		DESCRIPTION		"The number of WiFi session"

		::= {sHubWlanBasicConfig 15}

sHubWlanSessionLimit	OBJECT-TYPE
		SYNTAX			INTEGER
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"Limit of WiFi session"

		::= {sHubWlanBasicConfig 16}

sHubWlanBandAutoSet		OBJECT-TYPE
		SYNTAX			INTEGER { enable(1), disable(2) }
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION		"WLAN band auto configure

						Input Value
						===========
						1: Enable
						2: Disable"

		::= {sHubWlanBasicConfig 19}

sHubWlanConfigTable		OBJECT-TYPE
		SYNTAX 		SEQUENCE OF SHUBWlanConfigEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			""
		::= {sHubWlanBasicConfig 7}

sHubWlanConfigEntry 		OBJECT-TYPE
		SYNTAX 		SHUBWlanConfigEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
			""
		INDEX	{sHubWlanConfigIndex}
		::= {sHubWlanConfigTable 1}

SHUBWlanConfigEntry ::=
	SEQUENCE {
		sHubWlanConfigIndex		INTEGER,
		sHubWlanSSID			OCTET STRING,
		sHubWlanSSIDMode		INTEGER,
		sHubWlanBcastSSIDMode	INTEGER,
		sHubWlanSecEncrytion	INTEGER,
		sHubWlanSubDetailRateMode INTEGER,
		sHubWlanSubDetailTrafficTx INTEGER,
		sHubWlanSubDetailTrafficRx INTEGER

		}

sHubWlanConfigIndex		OBJECT-TYPE
		SYNTAX 		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION
			""
		::= {sHubWlanConfigEntry 1}

sHubWlanSSID			OBJECT-TYPE
		SYNTAX			OCTET STRING
